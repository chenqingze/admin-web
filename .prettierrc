{
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "trailingComma": "all",
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "angular"
            }
        }
    ]
}
